<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleDisplayName</key>
	<string>Cluck Farm: Emberloom</string>
	<key>CFBundleExecutable</key>
	<string>$(EXECUTABLE_NAME)</string>
	<key>CFBundleIdentifier</key>
	<string>com.cluckfarm.emberloom</string>
	<key>CFBundleLocalizations</key>
	<array>
		<string>en</string>
		<string>ru</string>
	</array>
	<key>CFBundleName</key>
	<string>Cluck Farm: Emberloom</string>
	<key>CFBundleShortVersionString</key>
	<string>1.0</string>
	<key>CFBundleVersion</key>
	<string>1</string>
	<key>NSCameraUsageDescription</key>
	<string>Cluck Farm uses camera to scan QR codes for special farm events and AR chicken interactions.</string>
	<key>NSMicrophoneUsageDescription</key>
	<string>Cluck Farm uses microphone to detect clucking sounds for interactive gameplay features.</string>
	<key>NSUserTrackingUsageDescription</key>
	<string>Cluck Farm tracks usage to provide personalized farm recommendations and improve your gaming experience.</string>
	<key>NSLocationWhenInUseUsageDescription</key>
	<string>Cluck Farm uses location to find nearby farm events and weather-based gameplay features.</string>
	<key>NSAppTransportSecurity</key>
	<dict>
		<key>NSAllowsArbitraryLoads</key>
		<true/>
		<key>NSExceptionDomains</key>
		<dict>
			<key>cluckfargame.com</key>
			<dict>
				<key>NSExceptionAllowsInsecureHTTPLoads</key>
				<true/>
				<key>NSExceptionMinimumTLSVersion</key>
				<string>TLSv1.0</string>
				<key>NSExceptionRequiresForwardSecrecy</key>
				<false/>
			</dict>
		</dict>
	</dict>
	<key>ITSAppUsesNonExemptEncryption</key>
	<false/>
	<key>NSUserTrackingUsageDescription</key>
	<string>This app would like to track you across apps and websites to provide personalized gaming experiences.</string>
</dict>
</plist>
